	local fadeInFrame = TweenService:Create(FailedFrame, TweenInfo.new(1.2), { BackgroundTransparency = 0 })
	local fadeInText1 = TweenService:Create(FailedTitle, TweenInfo.new(1.2), { TextTransparency = 0 })
	local fadeInText2 = TweenService:Create(FailedMessage, TweenInfo.new(1.2), { TextTransparency = 0 })

	fadeInFrame:Play()
