------------------------------------------------
-- Dots animation
------------------------------------------------
local dots = ""
local statusBase = "Bypassing anti-cheat"
task.spawn(function()
	while LoadingFrame.Parent do
		for i = 0, 3 do
			dots = string.rep(".", i)
			StatusText.Text = statusBase .. dots
			task.wait(0.35)
		end
	end
end)

------------------------------------------------
-- SMOOTH LOADING BAR ONLY
------------------------------------------------
task.spawn(function()
	local totalTime = 300 -- ðŸ”¥ 5 minutes loading time
	local startTime = tick()

	while tick() - startTime < totalTime do
		local progress = math.clamp((tick() - startTime) / totalTime, 0, 1)
		local tween = TweenService:Create(
			ProgressBar,
			TweenInfo.new(0.1, Enum.EasingStyle.Linear),
			{ Size = UDim2.new(progress, 0, 1, 0) }
		)
		tween:Play()
		PercentageText.Text = math.floor(progress * 100) .. "%"
		statusBase = progress < 0.5 and "Bypassing anti-cheat" or "Finalizing bypass"
		task.wait(0.05)
	end

	-- Full bar instantly
	ProgressBar.Size = UDim2.new(1, 0, 1, 0)
	PercentageText.Text = "100%"
	task.wait(0.3)

	------------------------------------------------
	-- REMOVE LOADING UI
	------------------------------------------------
	LoadingFrame:Destroy()

	------------------------------------------------
	-- FAILED UI (fade-in only)
	------------------------------------------------
