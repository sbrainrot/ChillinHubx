	FailedFrame.Size = UDim2.new(0, 340, 0, 120)
	FailedFrame.Position = UDim2.new(0.5, -170, 0.5, -60)
	FailedFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 35)
	FailedFrame.BorderSizePixel = 0
	FailedFrame.BackgroundTransparency = 1
	FailedFrame.Parent = gui
	Instance.new("UICorner", FailedFrame).CornerRadius = UDim.new(0, 10)

	local failGradient = Instance.new("UIGradient")
