		ColorSequenceKeypoint.new(0, Color3.fromRGB(10, 10, 25)),
		ColorSequenceKeypoint.new(1, Color3.fromRGB(35, 0, 70))
	})
	failGradient.Rotation = 90
	failGradient.Parent = FailedFrame

	local FailedTitle = Instance.new("TextLabel")
	FailedTitle.Parent = FailedFrame
	FailedTitle.Size = UDim2.new(1, 0, 0, 30)
	FailedTitle.Position = UDim2.new(0, 0, 0.15, 0)
	FailedTitle.BackgroundTransparency = 1
	FailedTitle.Text = "FAILED!"
	FailedTitle.Font = Enum.Font.GothamBold
	FailedTitle.TextColor3 = Color3.fromRGB(255, 80, 80)
	FailedTitle.TextSize = 22
